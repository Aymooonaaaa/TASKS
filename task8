#include <stdio.h>
#include <stdlib.h>
void task4()
{
     int x,i;
 printf("enter the number \t");
 scanf("%i",&x);
 for(int i=1;i<=(x/2);i++)

 {

  printf("%i \t",i);

 }
 printf("%i \t",x);
for(int i=((x/2)+1);i<x;i++)
{

    printf("%i \t",i);
}
printf("%i \t \a",(x*2));

}
int main()
{
   task4();
}
